{
  "name": "crypto-presentation",
  "version": "0.0.1",
  "dependencies": {
    "enigma": "1.1.2",
    "reveal.js": "3.5.0",
    "lodash": "4.17.4"
  },
  "devDependencies": {
    "browserify": "14.5.0",
    "watchify": "3.9.0",
    "minifyify": "7.3.5",
    "pug": "2.0.0-rc.4",
    "pug-cli": "1.0.0-alpha6",
    "jest": "21.2.1"
  },
  "scripts": {
    "build:js": "browserify src/js/index.js > dist/index.js",
    "build:pug": "pug src/index.pug --out dist",
